public class Main {
    public static void main(String[] args) {
        int a[]= {54,8,4457,2,3434,123,434,1,3};
        
        int max=a[0];
        int min=a[0];
        for(int i=1;i<a.length;i++){
            if(max<a[i]){
                max=a[i];
            }
            if(min>a[i]){
                min=a[i];
            }
        }
        
        System.out.println(max);
        System.out.println(min);
        
    }
}
